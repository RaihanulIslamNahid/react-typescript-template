    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "gh-pages": "^6.1.1",
    "prettier": "^3.2.5",
    "sass": "^1.71.1",
    "typescript": "^5.2.2",
    "vite": "^5.1.5"
